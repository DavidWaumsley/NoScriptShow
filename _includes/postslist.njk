<ol reversed>{%- for post in postslist | reverse %}
<li><a href="{{ post.url }}">{% if post.data.title %}{{ post.data.title }}{% else %}<code>{{ post.url }}</code>{% endif %}</a> - 
<small><time class="postlist-date" datetime="{{ post.date | htmlDateString }}">{{ post.date | readableDate(" MMM yyyy") }}</time></small>
</li>{%- endfor %}
</ol>